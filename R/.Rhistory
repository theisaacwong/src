scale_y_continuous(breaks = seq(from=1, to=1500, by=60), labels = c("12AM", paste0(1:11, "AM"), "12PM", paste0(1:11, "PM"), "12AM")) +
scale_x_continuous(breaks = df_heat$Var3  %>% u, labels = weekday_labels) +
