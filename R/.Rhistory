df_heat$Var2 <- sprintf("%02d", df_heat$Var2)
df_heat$key <- paste0(df_heat$Var2, ":", df_heat$Var1)
df_heat$month <- sprintf("%02d", df_heat$Var3)
df_heat$count <- lapply(seq_down(df_heat), function(i){
